{
  "name": "@samithahansaka/formless-formik",
  "version": "0.1.0",
  "description": "Formik adapter for Universal Form Adapter",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@samithahansaka/formless-core": "*"
  },
  "peerDependencies": {
    "formik": "^2.4.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "formik": "^2.4.6",
    "react": "^18.3.1",
    "tsup": "^8.5.1",
    "typescript": "^5.8.3",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "formik",
    "form",
    "forms",
    "formless",
    "react",
    "typescript",
    "adapter",
    "universal",
    "validation",
    "useFormik"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samithahansaka/formless.git",
    "directory": "packages/formik"
  }
}

{
  "name": "@universal-form/tanstack-form",
  "version": "0.1.0",
  "description": "TanStack Form adapter for Universal Form Adapter",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@universal-form/core": "*"
  },
  "peerDependencies": {
    "@tanstack/react-form": ">=0.40.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@tanstack/react-form": "^1.27.7",
    "react": "^18.3.1",
    "typescript": "^5.8.3",
    "tsup": "^8.5.1",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "tanstack",
    "tanstack-form",
    "form",
    "forms",
    "universal-form-adapter",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/universal-form-adapter/universal-form-adapter.git",
    "directory": "packages/tanstack-form"
  }
}
